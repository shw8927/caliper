{
    "blockchain": {
      "type": "composer",
      "config": "network/fabric-v11/2-org-1-peer/fund-composer-tls.json"
    },
    "command" : {
      "start": "docker-compose -f network/fabric-v11/2-org-1-peer/docker-compose-tls.yaml up -d",
      "end": "docker-compose -f network/fabric-v11/2-org-1-peer/docker-compose-tls.yaml down;docker rm $(docker ps -aq);docker rmi $(docker images dev-* -q)"
    },
    "test": {
      "name": "Fund-Clearing Performance test",
      "description" : "This is an example Composer perf test for clearing house",
      "clients": {
        "type": "local",
        "number": 1
      },
      "rounds": [{
                "label" : "fund-clearing-network",
                "label4Report" : "Fund-clearing-Invoke",
                "txNumber" : [10],
                "trim" : 0,
                "rateControl" : [{"type": "fixed-rate", "opts": {"tps": 0.2}}],
                "arguments": {"participantBankNumber":4,"transferReqPerBank": 7},
                "callback" : "benchmark/composer/composer-samples/fund-clearing-network.js"
              }
             
            ]
    },
    "monitor": {
      "type": ["docker", "process"],
      "docker":{
        "name": ["orderer.example.com","ca.org2.example.com","peer0.org1.example.com"]
      },
      "process": [
        {
          "command" : "node",
          "arguments" : "client.js",
          "multiOutput" : "avg"
        },
        {
          "command" : "node",
          "arguments" : "local-client.js",
          "multiOutput" : "avg"
        }
      ],
      "interval": 1
    }
  }
  
